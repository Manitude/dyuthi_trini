<h1>Listing tags</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Asset Name</th>
    <th>Action</th>
  </tr>
 
  <% @tags.each do |tag| %>
    <tr>
      <td><%= tag.name %></td>
      <td><%= Asset.find(tag.asset_id).name %></td>
      <td><%= link_to 'Show', tag_path(tag) %></td>
    </tr>
  <% end %>
</table>