<%= form_for :corporate, url: corporates_path, local: true do |form| %>
  
    <% if @corporate.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@corporate.errors.count, "error") %> prohibited
        this article from being saved:
      </h2>
      <ul>
        <% @corporate.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
 
  <p>
    <%= form.label :choose_license_package %><br>
    <%= form.select :license_package_id, LicensePackage.all.collect {|c| [ c.name, c.id ] } %><br>
    <br>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>